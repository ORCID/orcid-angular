          },
          "ja": {
            "translation": "src/locale/messages.static.ja.xlf",
            "baseHref": "/"
          },
          "ko": {
            "translation": "src/locale/messages.static.ko.xlf",
            "baseHref": "/"
          },
          "lr": {
            "translation": "src/locale/messages.static.lr.xlf",
            "baseHref": "/"
          },
          "pt": {
            "translation": "src/locale/messages.static.pt.xlf",
            "baseHref": "/"
