        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/orcid-ui-elements/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/orcid-ui-elements/public"
              }
            ],
            "styles": ["projects/orcid-ui-elements/src/styles.scss"]
          }
        }
      }
