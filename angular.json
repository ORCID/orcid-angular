{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-orcid": {
      "i18n": {
        "locales": {
          "en": {
            "translation": "src/locale/messages.static.en.xlf",
            "baseHref": "/"
          },
          "fr": {
            "translation": "src/locale/messages.static.fr.xlf",
            "baseHref": "/"
          },
          "ar": {
