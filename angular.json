            "baseHref": "/"
          },
          "xx": {
            "translation": "src/locale/messages.static.xx.xlf",
            "baseHref": "/"
          },
          "src": {
            "translation": "src/locale/messages.static.source.xlf",
            "baseHref": "/"
          },
          "zh-CN": {
            "translation": "src/locale/messages.static.zh_CN.xlf",
            "baseHref": "/"
          },
          "zh-TW": {
            "translation": "src/locale/messages.static.zh_TW.xlf",
            "baseHref": "/"
          }
        }
      },
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "crossOrigin": "use-credentials",
            "aot": true,
            "optimization": true,
            "outputHashing": "all",
            "sourceMap": false,
            "namedChunks": false,
            "extractLicenses": true,
            "vendorChunk": false,
            "buildOptimizer": true,
            "outputPath": "dist",
            "i18nMissingTranslation": "error",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets/",
                "ignore": ["fonts/**/*", "**/*.scss"],
                "output": "/assets/"
              },
              {
                "glob": "src/favicon.ico",
                "input": "src/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "src/assets/scss/noto-sans.scss",
              "src/assets/scss/material.scss",
              "src/assets/scss/orcid.scss",
              "src/styles.scss",
              "src/assets/scss/grid.scss"
            ],
            "fileReplacements": [
              {
                "replace": "src/environments/environment.ts",
                "with": "src/environments/environment.local.ts"
              }
            ]
          },
          "configurations": {
            "local": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": false,
              "outputPath": "dist"
            },
            "local-tomcat": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.tomcat.ts"
                }
              ]
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.production.ts"
                }
              ]
            },
            "qa": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.qa.ts"
                }
              ]
            },
            "sandbox": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.sandbox.ts"
                }
              ]
            },
			"int": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.int.ts"
                }
              ]
            },
			"en": {
              "localize": ["en"]
            },
            "fr": {
              "localize": ["fr"]
            },
            "ar": {
              "localize": ["ar"]
            },
            "ca": {
              "localize": ["ca"]
            },
            "cs": {
              "localize": ["cs"]
            },
            "es": {
              "localize": ["es"]
            },
            "it": {
              "localize": ["it"]
            },
            "ja": {
              "localize": ["ja"]
            },
            "ko": {
              "localize": ["ko"]
            },
            "lr": {
              "localize": ["lr"]
            },
            "pt": {
              "localize": ["pt"]
            },
            "rl": {
              "localize": ["rl"]
            },
            "ru": {
              "localize": ["ru"]
            },
            "uk": {
              "localize": ["uk"]
            },
            "xx": {
              "localize": ["xx"]
            },
            "src": {
              "localize": ["src"]
            },
            "zh-CN": {
              "localize": ["zh-CN"]
            },
            "zh-TW": {
              "localize": ["zh-TW"]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ng-orcid:build:local"
          },
          "configurations": {
            "local": {
              "browserTarget": "ng-orcid:build:local"
            },
            "local-tomcat": {
              "browserTarget": "ng-orcid:build:local-tomcat"
            },
            "production": {
              "browserTarget": "ng-orcid:build:production"
            },
            "sandbox": {
              "browserTarget": "ng-orcid:build:sandbox"
            },
			"int": {
              "browserTarget": "ng-orcid:build:int"
            },
            "qa": {
              "browserTarget": "ng-orcid:build:qa"
            },
            "en": {
              "browserTarget": "ng-orcid:build:en"
            },
            "fr": {
              "browserTarget": "ng-orcid:build:fr"
            },
            "ar": {
              "browserTarget": "ng-orcid:build:ar"
            },
            "ca": {
              "browserTarget": "ng-orcid:build:ca"
            },
            "cs": {
              "browserTarget": "ng-orcid:build:cs"
            },
            "es": {
              "browserTarget": "ng-orcid:build:es"
            },
            "it": {
              "browserTarget": "ng-orcid:build:it"
            },
            "ja": {
              "browserTarget": "ng-orcid:build:ja"
            },
            "ko": {
