{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-orcid": {
      "i18n": {
        "sourceLocale": {
          "code": "en",
          "baseHref": "/"
        },
        "locales": {
          "fr": {
            "translation": "src/locale/messages.fr.xlf",
            "baseHref": "/"
          },
          "ar": {
            "translation": "src/locale/messages.ar.xlf",
            "baseHref": "/"
          },
          "ca": {
            "translation": "src/locale/messages.ca.xlf",
            "baseHref": "/"
          },
          "cs": {
            "translation": "src/locale/messages.cs.xlf",
            "baseHref": "/"
          },
          "de": {
            "translation": "src/locale/messages.de.xlf",
            "baseHref": "/"
          },
          "es": {
            "translation": "src/locale/messages.es.xlf",
            "baseHref": "/"
          },
          "it": {
            "translation": "src/locale/messages.it.xlf",
            "baseHref": "/"
          },
          "ja": {
            "translation": "src/locale/messages.ja.xlf",
            "baseHref": "/"
          },
          "ko": {
            "translation": "src/locale/messages.ko.xlf",
            "baseHref": "/"
          },
          "lr": {
            "translation": "src/locale/messages.lr.xlf",
            "baseHref": "/"
          },
          "pl": {
            "translation": "src/locale/messages.pl_PL.xlf",
            "baseHref": "/"
          },
          "pt": {
            "translation": "src/locale/messages.pt.xlf",
            "baseHref": "/"
          },
          "rl": {
            "translation": "src/locale/messages.rl.xlf",
            "baseHref": "/"
          },
          "ru": {
            "translation": "src/locale/messages.ru.xlf",
            "baseHref": "/"
          },
          "tr": {
            "translation": "src/locale/messages.tr_TR.xlf",
            "baseHref": "/"
          },
          "uk": {
            "translation": "src/locale/messages.uk.xlf",
            "baseHref": "/"
          },
          "xx": {
            "translation": "src/locale/messages.xx.xlf",
            "baseHref": "/"
          },
          "src": {
            "translation": "src/locale/messages.source.xlf",
            "baseHref": "/"
          },
          "zh-CN": {
            "translation": "src/locale/messages.zh_CN.xlf",
            "baseHref": "/"
          },
          "zh-TW": {
            "translation": "src/locale/messages.zh_TW.xlf",
            "baseHref": "/"
          }
        }
      },
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "crossOrigin": "use-credentials",
            "outputHashing": "all",
            "outputPath": "dist",
            "i18nMissingTranslation": "warning",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets/",
                "ignore": ["fonts/**/*", "**/*.scss"],
                "output": "/assets/"
              },
              {
                "glob": "src/favicon.ico",
                "input": "src/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "src/assets/css/tailwind.css",
              "src/assets/scss/noto-sans.scss",
              "src/assets/scss/material.scss",
              "projects/orcid-tokens/tokens.css",
              "src/assets/scss/orcid-ui-theme.scss",
              "src/assets/scss/orcid.scss",
              "src/styles.scss",
              "src/assets/scss/grid.scss"
            ],
            "scripts": [
              "/scripts/environment.runtime.js",
              "/scripts/onetrust.runtime.js"
            ]
          },
          "configurations": {
            "local": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ],
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "vendor": false,
                "hidden": false
              },
              "namedChunks": true,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": false,
              "outputPath": "dist"
            },
            "local-with-proxy": {
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": false,
              "outputPath": "dist",
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "local-with-proxy-localize-en": {
              "localize": ["en"],
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": false,
              "outputPath": "dist",
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "local-with-proxy-localize-fr": {
              "localize": ["fr"]
            },
            "local-with-proxy-localize-ar": {
              "localize": ["ar"]
            },
            "local-with-proxy-localize-xx": {
              "localize": ["xx"]
            },
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            },
            "en": {
              "localize": ["en"]
            },
            "fr": {
              "localize": ["fr"]
            },
            "ar": {
              "localize": ["ar"]
            },
            "ca": {
              "localize": ["ca"]
            },
            "cs": {
              "localize": ["cs"]
            },
            "es": {
              "localize": ["es"]
            },
            "it": {
              "localize": ["it"]
            },
            "ja": {
              "localize": ["ja"]
            },
            "ko": {
              "localize": ["ko"]
            },
            "lr": {
              "localize": ["lr"]
            },
            "pt": {
              "localize": ["pt"]
            },
            "rl": {
              "localize": ["rl"]
            },
            "ru": {
              "localize": ["ru"]
            },
            "uk": {
              "localize": ["uk"]
            },
            "xx": {
              "localize": ["xx"]
            },
            "src": {
              "localize": ["src"]
            },
            "zh-CN": {
              "localize": ["zh-CN"]
            },
            "zh-TW": {
              "localize": ["zh-TW"]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "ng-orcid:build:local"
          },
          "configurations": {
            "local-qa": {
              "proxyConfig": "src/proxy.conf.qa.mjs",
              "buildTarget": "ng-orcid:build:local-with-proxy"
            },
            "local-qa-en": {
              "proxyConfig": "src/proxy.conf.qa.mjs",
              "buildTarget": "ng-orcid:build:local-with-proxy-localize-en"
            },
            "local-qa-ar": {
              "proxyConfig": "src/proxy.conf.qa.mjs",
              "buildTarget": "ng-orcid:build:local-with-proxy-localize-ar"
            },
            "local-qa-fr": {
              "proxyConfig": "src/proxy.conf.qa.mjs",
              "buildTarget": "ng-orcid:build:local-with-proxy-localize-fr"
            },
            "local-qa-xx": {
              "proxyConfig": "src/proxy.conf.qa.mjs",
              "buildTarget": "ng-orcid:build:local-with-proxy-localize-xx"
            },
            "local-sandbox": {
              "proxyConfig": "src/proxy.conf.sandbox.mjs",
              "buildTarget": "ng-orcid:build:local-with-proxy"
            },
            "local-orcid-web": {
              "proxyConfig": "src/proxy.conf.orcid-web.mjs",
              "buildTarget": "ng-orcid:build:local-with-proxy"
            },
            "local": {
              "buildTarget": "ng-orcid:build:local"
            },
            "local-tomcat": {},
            "production": {
              "buildTarget": "ng-orcid:build:production"
            },

            "en": {
              "buildTarget": "ng-orcid:build:en"
            },
            "fr": {
              "buildTarget": "ng-orcid:build:fr"
            },
            "ar": {
              "buildTarget": "ng-orcid:build:ar"
            },
            "ca": {
              "buildTarget": "ng-orcid:build:ca"
            },
            "cs": {
              "buildTarget": "ng-orcid:build:cs"
            },
            "es": {
              "buildTarget": "ng-orcid:build:es"
            },
            "it": {
              "buildTarget": "ng-orcid:build:it"
            },
            "ja": {
              "buildTarget": "ng-orcid:build:ja"
            },
            "ko": {
              "buildTarget": "ng-orcid:build:ko"
            },
            "lr": {
              "buildTarget": "ng-orcid:build:lr"
            },
            "pt": {
              "buildTarget": "ng-orcid:build:pt"
            },
            "rl": {
              "buildTarget": "ng-orcid:build:rl"
            },
            "ru": {
              "buildTarget": "ng-orcid:build:ru"
            },
            "uk": {
              "buildTarget": "ng-orcid:build:uk"
            },
            "xx": {
              "buildTarget": "ng-orcid:build:xx"
            },
            "src": {
              "buildTarget": "ng-orcid:build:src"
            },
            "zh-CN": {
              "buildTarget": "ng-orcid:build:zh-CN"
            },
            "zh-TW": {
              "buildTarget": "ng-orcid:build:zh-TW"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "ng-orcid:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "projects/orcid-tokens/tokens.css",
              "src/assets/scss/orcid-ui-theme.scss",
              "src/styles.scss"
            ],
            "scripts": [
              "/scripts/environment.runtime.js",
              "/scripts/onetrust.runtime.js"
            ],
            "assets": ["src/favicon.ico", "src/assets", "src/manifest.json"],
            "sourceMap": true
          }
        }
      }
    },
    "ng-orcid-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ng-orcid:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ng-orcid:serve:production"
            }
          }
        }
      }
    },
    "orcid-ui": {
      "projectType": "library",
      "root": "projects/orcid-ui",
      "sourceRoot": "projects/orcid-ui/src",
      "prefix": "orcid",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/orcid-ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/orcid-ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/orcid-ui/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "orcid-tokens": {
      "projectType": "library",
      "root": "projects/orcid-tokens",
      "sourceRoot": "projects/orcid-tokens/src",
      "prefix": "orcid",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/orcid-tokens/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/orcid-tokens/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/orcid-tokens/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "orcid-ui-docs": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/orcid-ui-docs",
      "sourceRoot": "projects/orcid-ui-docs/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/orcid-ui-docs",
            "browser": "projects/orcid-ui-docs/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/orcid-ui-docs/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/orcid-ui-docs/public"
              }
            ],
            "styles": ["projects/orcid-ui-docs/src/styles.scss"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kB",
                  "maximumError": "15kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "orcid-ui-docs:build:production"
            },
            "development": {
              "buildTarget": "orcid-ui-docs:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/orcid-ui-docs/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/orcid-ui-docs/public"
              }
            ],
            "styles": ["projects/orcid-ui-docs/src/styles.scss"]
          }
        }
      }
    },
    "orcid-ui-elements": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/orcid-ui-elements",
      "sourceRoot": "projects/orcid-ui-elements/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/orcid-ui-elements/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/orcid-ui-elements/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/orcid-ui-elements/public"
              }
            ],
            "styles": ["projects/orcid-ui-elements/src/styles.scss"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "orcid-ui-elements:build:production"
            },
            "development": {
              "buildTarget": "orcid-ui-elements:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
